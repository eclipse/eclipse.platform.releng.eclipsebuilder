<project default="noDefault">

	<target name="noDefault" />

	<!-- This goal of this target is to get the packaging map files of the things you care for 
        The files must be fetched into the downloadDirectory folder -->
	<target name="getMapFiles">
	</target>

	<target name="assemble.delta.delta.delta.xml">
		<ant antfile="${assembleScriptName}">
			<property name="archiveName" value="eclipse-RCP-${buildId}-delta-pack.zip" />
		</ant>
	</target>
	<target name="assemble.group.group.group.xml">
		<ant antfile="${assembleScriptName}">
			<property name="archiveName" value="eclipse-RCP-${buildId}-grouped-delta-pack.zip" />
		</ant>
	</target>
</project>
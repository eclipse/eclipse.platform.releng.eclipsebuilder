#!/bin/sh

mkdir $WORKSPACE/builds
cd $WORKSPACE/builds
mkdir -p $WORKSPACE/builds/N
mkdir -p $WORKSPACE//builds/transfer/files/testUpdates-N
$WORKSPACE/bootstrapHudson.sh -test -skipTest -buildDirectory $WORKSPACE/builds/N -skipRSS -hudson -skipPack -updateSite $WORKSPACE/builds/transfer/files/testUpdates-N N
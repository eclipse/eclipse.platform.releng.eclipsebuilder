#!/bin/sh

mkdir $WORKSPACE/builds
cd $WORKSPACE/builds
mkdir -p $WORKSPACE/builds/I
mkdir -p $WORKSPACE/builds/transfer/files/testUpdates-I
$WORKSPACE/org.eclipse.releng.eclipsebuilder/bootstrapHudsonp2.sh -test -skipTest -buildDirectory $WORKSPACE/builds/I -hudson -updateSite $WORKSPACE/builds/transfer/files/testUpdates-I I